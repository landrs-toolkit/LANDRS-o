@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix wgs: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix foaf: <http://xmlns.com/foaf/spec/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix tagg: <https://www.w3.org/2006/time/agg#> .
@prefix landrs: <http://schema.landrs.org/schema/> .
@prefix landrsrdf: <http://schema.landrs.org/rdf/> .
@prefix landrso: <https://schema.landrs.org/rdf/ontology/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix ssn-ext: <http://www.w3.org/ns/ssn/ext/> .
@base <http://schema.landrs.org/> .


foaf:Agent a owl:Class .
foaf:name a owl:AnnotationProperty .
voaf:Vocabulary a owl:Class .
dcterms:title a owl:AnnotationProperty .
dcterms:description a owl:AnnotationProperty .
dcterms:rights a owl:AnnotationProperty .
dcterms:license a owl:AnnotationProperty .
dcterms:created a owl:AnnotationProperty .
dcterms:creator a owl:AnnotationProperty .
skos:definition a owl:AnnotationProperty .
opla:isNativeTo a owl:AnnotationProperty .
opla:reusesPatternAsTemplate a owl:AnnotationProperty .
skos:example a owl:AnnotationProperty .
skos:note a owl:AnnotationProperty .
vann:preferredNamespacePrefix a owl:AnnotationProperty .
vann:preferredNamespaceUri a owl:AnnotationProperty .
schema:domainIncludes a owl:AnnotationProperty .
schema:rangeIncludes a owl:AnnotationProperty .
schema:version a owl:AnnotationProperty .


# URI for the fully dereferencable ontology
<http://schema.landrs.org/rdf> a owl:Ontology , voaf:Vocabulary, prov:Entity, dcat:Dataset ;
  dcterms:title "Landrs Schema"@en ;
  dcterms:description "This ontology is developed as part of the Sloan Funded LANDRS project to advance UxV data interoperability"@en ;
  dcterms:creator [ a foaf:Agent, prov:Agent ; foaf:name "LANDRS: Linked and Networked Drones"@en ] ;
  dcterms:rights "Copyright 2020 University of Notre Dame." ;
  dcterms:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
  dcterms:created "2020-02-01"^^xsd:date ;
  vann:preferredNamespacePrefix "landrs" ;
  vann:preferredNamespaceUri "http://schema.landrs.org/schema/" ;
  owl:versionInfo "0.0.1" ;
  schema:versionInfo "0.0.1" ;
  opla:reusesPatternAsTemplate <https://raw.githubusercontent.com/cogan-shimizu-wsu/WinstonPartWhole/master/WinstonPartWhole.owl> ;
  dcat:distribution [
    dcat:accessURL <http://schema.landrs.org/rdf/ontology/> ;
    dcat:mediaType <https://www.iana.org/assignments/media-types/application/ld+json> ;
    dcat:mediaType <https://www.iana.org/assignments/media-types/application/n-triples>
  ] ;
  owl:imports landrsrdf: .


#################################################################
#    Object Properties
#################################################################

###  http://schema.landrs.org/schema/hadPlan
landrs:hadPlan rdf:type owl:ObjectProperty ;
           rdfs:domain landrs:DataPlanAssociation ;
           rdfs:range landrs:DataCapturePlan ;
           rdfs:label "hadPlan" .


###  http://schema.landrs.org/schema//hasDataSource
landrs:hasDataSource rdf:type owl:ObjectProperty ;
                 rdfs:domain landrs:DataSource ;
                 rdfs:range landrs:DataSource ;
                 rdfs:label "hasDataSource" .


###  http://schema.landrs.org/schema/hasSpatioTemporalExtent
landrs:hasSpatioTemporalExtent rdf:type owl:ObjectProperty ;
                           rdfs:domain landrs:Flight ;
                           rdfs:range landrs:SpatialTemporalAggregate ;
                           rdfs:label "hasSpatioTemporalExtent" .


###  http://schema.landrs.org/schema/producedBy
landrs:producedBy rdf:type owl:ObjectProperty ;
              rdfs:domain landrs:DataSource ;
              rdfs:range landrs:SensorPayload ;
              rdfs:label "producedBy" .


###  http://schema.landrs.org/schema/provides
landrs:provides rdf:type owl:ObjectProperty ;
            rdfs:domain landrs:SensorPayload ;
            rdfs:range landrs:DataSource ;
            rdfs:label "provides" .


###  http://schema.landrs.org/schema/servesTrajectory
landrs:servesTrajectory rdf:type owl:ObjectProperty ;
                    rdfs:domain landrs:Trajectorysource ;
                    rdfs:range landrs:TrajectoryObservationCollection ;
                    rdfs:label "servesTrajectory" .

# New properties
###  http://schema.landrs.org/schema/location
landrs:location rdf:type owl:ObjectProperty ;          
                rdfs:domain landrs:Event ;
                rdfs:range landrs:Place ;
                rdfs:label "location" .


###  http://schema.landrs.org/schema/providesParticipantRole
landrs:providesParticipantRole rdf:type owl:ObjectProperty ;
                           rdfs:domains landrs:Event ;
                           rdfs:range landrs:ParticipantRole ;
                           rdfs:label "providesParticipantRole" .


#################################################################
#    Classes
#################################################################

### http://schema.landrs.org/schema/DataAquisition
landrs:DataAquisition rdf:type  owl:Class ;
                      rdfs:subClassOf
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty landrs:qualifiedAssociation ;
                                  owl:someValuesFrom landrs:DataplanAssociation
                                ] ;
                      rdfs:isDefinedBy landrsrdf: .


###  http://schema.landrs.org/schema/DataCapturePlan
landrs:DataCapturePlan  rdf:type  owl:Class ;
                        rdfs:subClassOf
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty landrs:wasGeneratedBy ;
                                    owl:someValuesFrom landrs:DataCapturePlanningActivity
                                  ] ;
                        rdfs:isDefinedBy landrsrdf: .


###  http://schema.landrs.org/schema/DataCapturePlanningActivity
landrs:DataCapturePlanningActivity  rdf:type owl:Class ;
                                    rdfs:isDefinedBy landrsrdf: .


###  http://schema.landrs.org/schema/DataPlanAssociation
landrs:DataPlanAssociation  rdf:type  owl:Class ;
                            rdfs:subClassOf
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty landrs:hadPlan ;
                                        owl:someValuesFrom landrs:DataCapturePlan
                                      ] ;
                            rdfs:isDefinedBy landrsrdf: .


###  http://schema.landrs.org/schema/Flight
landrs:Flight rdf:type  owl:Class ;
              rdfs:subClassOf
                        [ rdf:type owl:Restriction ;
                          owl:onProperty landrs:qualifiedAssociation ;
                          owl:someValuesFrom landrs:FlightPlanAssociation
                        ] ;
              rdfs:isDefinedBy landrsrdf: .


###  http://schema.landrs.org/schema/FlightPlan
landrs:FlightPlan rdf:type  owl:Class ;
                  rdfs:subClassOf
                            [ rdf:type owl:Restriction ;
                              owl:onProperty landrs:wasGeneratedBy ;
                              owl:someValuesFrom landrs:FlightPlanningActivity
                            ] ;
                  rdfs:isDefinedBy landrsrdf: .


###  http://schema.landrs.org/schema/FlightPlanAssociation
landrs:FlightPlanAssociation  rdf:type  owl:Class ;
                              rdfs:subClassOf
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty landrs:hadPlan ;
                                          owl:someValuesFrom landrs:FlightPlan
                                        ] ;
                              rdfs:isDefinedBy landrsrdf: .


###  http://schema.landrs.org/schema/FlightPlanningActivity
landrs:FlightPlanningActivity rdf:type owl:Class ;
                              rdfs:isDefinedBy landrsrdf: .


###  http://schema.landrs.org/schema/Pilot
landrs:Pilot  rdf:type owl:Class ;
              rdfs:isDefinedBy landrsrdf: .


###  http://schema.landrs.org/schema/Plan
landrs:Plan rdf:type owl:Class ;
            rdfs:isDefinedBy landrsrdf: .


###  http://schema.landrs.org/schema/Role
lndrs:Role  rdf:type owl:Class ;
            rdfs:isDefinedBy landrsrdf: .

###  http://schema.landrs.org/schema/DataSource
landrs:DataSource rdf:type owl:Class ;
              rdfs:isDefinedBy landrsrdf: .

###  http://schema.landrs.org/schema/ObservationCollection
landrs:ObservationCollection  rdf:type owl:Class ;
                          rdfs:isDefinedBy landrsrdf: .


###  http://schema.landrs.org/schema/SensorPayload
landrs:SensorPayload  rdf:type owl:Class ;
                  rdfs:isDefinedBy landrsrdf: .


###  http://schema.landrs.org/schema/SpatialTemporalAggregate
landrs:SpatialTemporalAggregate rdf:type owl:Class ;
                            rdfs:isDefinedBy landrsrdf: .


###  http://schema.landrs.org/schema/TrajectoryObservationCollection
landrs:TrajectoryObservationCollection  rdf:type owl:Class ;
                                    rdfs:isDefinedBy landrsrdf: .


###  http://schema.landrs.org/schema/TrajectorySource
landrs:TrajectorySource rdf:type owl:Class ;
                    rdfs:isDefinedBy landrsrdf: .

 # New classes
###  http://schema.landrs.org/schema/Event
landrs:Event  rdf:type owl:Class ;
              rdfs:isDefinedBy landrsrdf: .

 ###  http://schema.landrs.org/schema/Place
landrs:Place  rdf:type owl:Class ;
              rdfs:isDefinedBy landrsrdf: .

###  http://schema.landrs.org/schema/ParticipantRole
landrs:ParticipantRole rdf:type owl:Class ;
              rdfs:isDefinedBy landrsrdf: .
