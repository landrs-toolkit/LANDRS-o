@prefix : <http://schema.landrs.org/schema/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix wgs: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix foaf: <http://xmlns.com/foaf/spec/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix tagg: <https://www.w3.org/2006/time/agg#> .
@prefix landrs: <http://schema.landrs.org/schema/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix ssn-ext: <http://www.w3.org/ns/ssn/ext/> .
@prefix schema: <https://schema.org/> .
@base <http://schema.landrs.org/> .


#################################################################
#    Object Properties
#################################################################

###  https://schema.landrs.org/schema/hadPlan
prov:hadPlan  rdf:type rdf:Property ;
              rdfs:label "hadPlan" .


###  https://schema.landrs.org/schema/hadRole
prov:hadRole  rdf:type rdf:Property ;
              rdfs:domain landrs:FlightPlanAssociation ;
              rdfs:range landrs:Pilot ;
              rdfs:label "hadRole" .


###  http://www.w3.org/ns/prov#qualifiedAssociation
prov:qualifiedAssociation rdf:type rdf:Property ;
                          rdfs:domain landrs:DataAquisition ;
                          rdfs:range landrs:DataPlanAssociation ;
                          rdfs:label "prov:qualifiedAssociation" .

###  https://schema.landrs.org/schema/wasGeneratedBy
prov:wasGeneratedBy rdf:type rdf:Property ;
                    rdfs:label "wasGeneratedBy" .

###  http://www.w3.org/ns/prov#used
prov:used rdf:type rdf:Property ;
          rdfs:domain landrs:DataAquisition ;
          rdfs:range landrs:DataSource ;
          rdfs:label "prov:used" .


###  http://www.w3.org/ns/prov#wasGeneratedBy
prov:wasGeneratedBy rdf:type rdf:Property ;
                    rdfs:domain landrs:ObservationCollection ;
                    rdfs:range landrs:DataAquisition ;
                    rdfs:label "prov:wasGeneratedBy" .


###  http://www.w3.org/ns/prov#wasInformedBy
prov:wasInformedBy rdf:type rdf:Property ;
                   rdfs:domain landrs:DataAquisition ;
                   rdfs:range landrs:Flight ;
                   rdfs:label "prov:wasInformedBy" .


##  http://purl.org/dc/terms/format
dct:format rdf:type rdf:Property ;
           rdfs:domain landrs:DataSource ;
           rdfs:range dct:MediaTypeOrExtent ;
           rdfs:label "dct:format" .


###  http://www.w3.org/ns/dcat#frequency
dcat:frequency rdf:type rdf:Property ;
               rdfs:domain landrs:DataSource ;
               rdfs:range dct:Frequency ;
               rdfs:label "dcat:frequency" .


###  http://www.w3.org/ns/dcat#servesDataset
dcat:servesDataset rdf:type rdf:Property ;
                   rdfs:domain landrs:DataSource ;
                   rdfs:range landrs:ObservationCollection ;
                   rdfs:label "dcat:servesDataset" .

### LANDRS Object Properties

###  http://schema.landrs.org/schema/component
landrs:component rdf:type rdf:Property ;
             schema:domainIncludes landrs:SensorPayload ;
             schema:rangeIncludes landrs:Uav ;
             rdfs:label "component" .


###  http://schema.landrs.org/schema/hadPlan
landrs:hadPlan rdf:type rdf:Property;
           schema:domainIncludes landrs:DataPlanAssociation ;
           schema:rangeIncludes landrs:DataCapturepPlan ;
           rdfs:label "hadPlan" .


###  http://schema.landrs.org/schema/hasDataSource
landrs:hasDataSource rdf:type rdf:Property ;
                 schema:domainIncludes landrs:DataSource ;
                 schema:rangeIncludes  landrs:DataSource ;
                 rdfs:label "hasDataSource" .


###  http://schema.landrs.org/schema/hasSpatioTemporalExtent
landrs:hasSpatioTemporalExtent rdf:type rdf:Property ;
                           schema:domainIncludes landrs:Flight ;
                           schema:rangeIncludes landrs:SpatialTemporalAggregate ;
                           rdfs:label "hasSpatioTemporalExtent" .

###  http://schema.landrs.org/schema/producedBy
landrs:producedBy rdf:type rdf:Property ;
              schema:domainIncludes landrs:DataSource ;
              schema:rangeIncludes landrs:SensorPayload ;
              rdfs:label "producedBy" .


###  http://schema.landrs.org/schema/provides
landrs:provides rdf:type rdf:Property ;
            schema:domainIncludes landrs:SensorPayload ;
            schema:rangeIncludes landrs:DataSource ;
            rdfs:label "provides" .


###  http://schema.landrs.org/schema/servesTrajectory
landrs:servesTrajectory rdf:type rdf:Property  ;
                        schema:domainIncludes landrs:TrajectorySource ;
                        schema:rangeIncludes landrs:TrajectoryObservationCollection ;
                        rdfs:label "servesTrajectory" .


# New properties
###  http://schema.landrs.org/schema/location
landrs:location rdf:type rdf:Property  ;
                rdfs:subPropertyOf schema:location ;                     
                schema:domainIncludes landrs:Event ;
                schema:rangeIncludes  landrs:Place ;
                rdfs:label "location" .

###  http://schema.landrs.org/schema/providesParticipantRole
landrs:providesParticipantRole rdf:type rdf:Property ;
                           schema:domainIncludes landrs:Event ;
                           schema:rangeIncludes landrs:ParticipantRole ;
                           rdfs:label "providesParticipantRole" .


#################################################################
#    Classes
#################################################################

###  http://www.w3.org/ns/dcat#Dataset
dcat:Dataset rdf:type rdfs:Class ;
             rdfs:label "dcat:Dataset" .


###  http://www.w3.org/ns/prov#Activity
prov:Activity rdf:type rdfs:Class ;
              rdfs:label "prov:Activity" .


###  http://www.w3.org/ns/prov#Association
prov:Association rdf:type rdfs:Class ;
                 rdfs:label "prov:Association" .


###  http://www.w3.org/ns/prov#Plan
prov:Plan rdf:type rdfs:Class ;
          rdfs:label "prov:Plan" .


###  http://www.w3.org/ns/prov#Role
prov:Role rdf:type rdfs:Class ;
          rdfs:label "prov:Role" .


### LANDRS Classes

###  https://schema.landrs.org/schema/DataAquisition
landrs:DataAquisition rdf:type rdfs:Class ;
                      rdfs:subClassOf prov:Activity ;
                      rdfs:label "DataAquisition" .


###  https://schema.landrs.org/schema/DataCapturePlan
landrs:DataCapturePlan  rdf:type rdfs:Class ;
                        rdfs:subClassOf landrs:Plan ;
                        rdfs:label "DataCapturePlan" .


###  https://schema.landrs.org/schema/DataCapturePlanningActivity
landrs:DataCapturePlanningActivity  rdf:type rdfs:Class ;
                                    rdfs:subClassOf prov:Activity ;
                                    rdfs:label "DataCapturePlanningActivity" .


###  https://schema.landrs.org/schema/DataPlanAssociation
landrs:DataPlanAssociation  rdf:type rdfs:Class ;
                            rdfs:subClassOf prov:Association ;
                            rdfs:label "DataPlanAssociation" .


###  https://schema.landrs.org/schema/FlightPlan
landrs:FlightPlan rdf:type rdfs:Class ;
                  rdfs:subClassOf landrs:Plan ;
                  rdfs:label "FlightPlan" .


###  https://schema.landrs.org/schema/FlightPlanAssociation
landrs:FlightPlanAssociation  rdf:type rdfs:Class ;
                              rdfs:subClassOf prov:Association ;
                              rdfs:label "FlightPlanAssociation" .


###  https://schema.landrs.org/schema/FlightPlanningActivity
landrs:FlightPlanningActivity rdf:type rdfs:Class ;
                              rdfs:subClassOf prov:Activity ;
                              rdfs:label "FlightPlanningActivity" .


###  https://schema.landrs.org/schema/Pilot
landrs:Pilot  rdf:type rdfs:Class ;
              rdfs:subClassOf landrs:Role ;
              rdfs:label "Pilot" .


###  https://schema.landrs.org/schema/Plan
landrs:Plan rdf:type rdfs:Class ;
            rdfs:subClassOf dcat:Dataset ,
                            prov:Plan ;
            rdfs:comment "a description having an explicit goal, to be achieved by executing the plan" ;
            rdfs:label "Plan" .


###  https://schema.landrs.org/schema/Role
landrs:Role rdf:type rdfs:Class ;
            rdfs:subClassOf prov:Role ;
            rdfs:label "Role" .

###  http://schema.landrs.org/schema/DataSource
landrs:DataSource rdf:type rdfs:Class;
              rdfs:subClassOf dcat:DataService ;
              rdfs:comment "Any source of observational data from a sensor or sensor system hosted by a drone and is provided by some software API or API service" ;
              rdfs:label "Data Source" .


###  http://schema.landrs.org/schema/DataAquisition
landrs:DataAquisition rdf:type rdfs:Class ;
                  rdfs:label "DataAquisition" .


###  http://schema.landrs.org/schema/DataCapturePlan
landrs:DataCapturePlan rdf:type rdfs:Class ;
                   rdfs:label "DataCapturePlan" .


###  http://schema.landrs.org/schema/DataPlanAssociation
landrs:DataPlanAssociation rdf:type rdfs:Class ;
                       rdfs:label "DataPlanAssociation" .


###  http://schema.landrs.org/schema/Event
landrs:Event rdf:type rdfs:Class ;
         rdfs:comment "Event from A Core Pattern for Events" ;
         rdfs:label "Event" .

###  https://schema.landrs.org/schema/Flight
landrs:Flight rdf:type rdfs:Class ;
          rdfs:subClassOf landrs:Event ,
                          prov:Activity ;
          rdfs:label "Flight" .


###  http://schema.landrs.org/schema/ObservationCollection
landrs:ObservationCollection rdf:type rdfs:Class ;
                         rdfs:subClassOf dcat:Dataset ,
                                         prov:Entity ,
                                         ssn-ext:ObservationCollection ;
                         rdfs:label "Observation Collection" .


###  http://schema.landrs.org/schema/SensorPayload
landrs:SensorPayload rdf:type rdfs:Class ;
                 rdfs:subClassOf ssn:System ;
                 rdfs:comment "Hardware Component of a drone system that provides a data source" ;
                 rdfs:label "Sensor Payload" .


###  http://schema.landrs.org/schema/SpatialTemporalAggregate
landrs:SpatialTemporalAggregate rdf:type rdfs:Class ;
                            rdfs:subClassOf wgs:SpatialThing ,
                                            tagg:TemporalAggregate ;
                            rdfs:label "SpatialTemporalAggregate" .


###  http://schema.landrs.org/schema/TrajectoryObservationCollection
landrs:TrajectoryObservationCollection rdf:type rdfs:Class ;
                                   rdfs:subClassOf landrs:ObservationCollection ,
                                                   landrs:SpatialTemporalAggregate ;
                                   rdfs:label "TrajectoryObservationCollection" .


###  http://schema.landrs.org/schema/TrajectorySource
landrs:TrajectorySource rdf:type rdfs:Class ;
                    rdfs:subClassOf landrs:DataSource ;
                    rdfs:label "TrajectorySource" .


###  http://schema.landrs.org/schema/Uav
landrs:Uav rdf:type rdfs:Class ;
       rdfs:label "UAV" .
  
# New classes
###  http://schema.landrs.org/schema/Event
landrs:Event rdf:type rdfs:Class ;
                  rdfs:subClassOf schema:Event ;
                  rdfs:label "Event" .

 ###  http://schema.landrs.org/schema/Place
landrs:Place rdf:type rdfs:Class ;
                  rdfs:subClassOf schema:Place ;
                  rdfs:label "Place" .

###  http://schema.landrs.org/schema/ParticipantRole
landrs:ParticipantRole rdf:type rdfs:Class ;
                  rdfs:label "ParticipantRole" .
