@prefix : <http://schema.landrs.org/schema/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix gf: <http://schema.landrs.org/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix wgs: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix foaf: <http://xmlns.com/foaf/spec/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix tagg: <https://www.w3.org/2006/time/agg#> .
@prefix landrs: <http://schema.landrs.org/schema/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix ssn-ext: <http://www.w3.org/ns/ssn/ext/> .
@prefix foaf: <http://xmlns.com/foaf/spec/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix tagg: <https://www.w3.org/2006/time/agg#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix ssn-ext: <http://www.w3.org/ns/ssn/ext/> .
@base <http://schema.landrs.org/> .



#################################################################
#    Object Properties
#################################################################

###  https://schema.landrs.org/schema//hadplan
prov:hadPlan rdf:type owl:ObjectProperty ;
                    rdfs:label "hadPlan" .


###  https://schema.landrs.org/schema//hadrole
prov:hadRole rdf:type owl:ObjectProperty ;
                                            rdfs:domain landrs:FlightPlanAssociation ;
                                            rdfs:range <https://schema.landrs.org/schema//Pilot> ;
                                            rdfs:label "hadRole" .


###  http://www.w3.org/ns/prov#qualifiedAssociation
prov:qualifiedAssociation rdf:type owl:ObjectProperty ;
                          rdfs:domain gf:Dataaquisition ;
                          rdfs:range gf:Dataplanassociation ;
                          rdfs:label "prov:qualifiedAssociation" .

###  https://schema.landrs.org/schema//wasgeneratedb
prov:wasGeneratedBy rdf:type owl:ObjectProperty ;
                                                   rdfs:label "wasGeneratedBy" .

###  http://www.w3.org/ns/prov#used
prov:used rdf:type owl:ObjectProperty ;
          rdfs:domain gf:Dataaquisition ;
          rdfs:range gf:DataSource ;
          rdfs:label "prov:used" .


###  http://www.w3.org/ns/prov#wasGeneratedBy
prov:wasGeneratedBy rdf:type owl:ObjectProperty ;
                    rdfs:domain gf:ObservationCollection ;
                    rdfs:range gf:Dataaquisition ;
                    rdfs:label "prov:wasGeneratedBy" .


###  http://www.w3.org/ns/prov#wasInformedBy
prov:wasInformedBy rdf:type owl:ObjectProperty ;
                   rdfs:domain gf:Dataaquisition ;
                   rdfs:range gf:Flight ;
                   rdfs:label "prov:wasInformedBy" .


##  http://purl.org/dc/terms/format
dct:format rdf:type owl:ObjectProperty ;
           rdfs:domain gf:DataSource ;
           rdfs:range dct:MediaTypeOrExtent ;
           rdfs:label "dct:format" .


###  http://www.w3.org/ns/dcat#frequency
dcat:frequency rdf:type owl:ObjectProperty ;
               rdfs:domain gf:DataSource ;
               rdfs:range dct:Frequency ;
               rdfs:label "dcat:frequency" .


###  http://www.w3.org/ns/dcat#servesDataset
dcat:servesDataset rdf:type owl:ObjectProperty ;
                   rdfs:domain gf:DataSource ;
                   rdfs:range gf:ObservationCollection ;
                   rdfs:label "dcat:servesDataset" .

### LANDRS Object Properties

###  http://schema.landrs.org/component
gf:component rdf:type rdf:Property ;
             schema:domainIncludes gf:SensorPayload ;
             schema:rangeIncludes gf:Uav ;
             rdfs:label "component" .


###  http://schema.landrs.org/hadplan
gf:hadplan rdf:type rdf:Property ;
           schema:domainIncludes gf:Dataplanassociation ;
           schema:rangeIncludes gf:Datacaptureplan ;
           rdfs:label "hadPlan" .


###  http://schema.landrs.org/hasdatasource
gf:hasdatasource rdf:type owl:ObjectProperty ;
                 rdfs:domain gf:DataSource ;
                 rdfs:range gf:DataSource ;
                 rdfs:label "hasDataSource" .


###  http://schema.landrs.org/hasspatiotemporalextent
gf:hasspatiotemporalextent rdf:type owl:ObjectProperty ;
                           rdfs:domain gf:Flight ;
                           rdfs:range gf:Spatialtemporalaggregate ;
                           rdfs:label "hasSpatioTemporalExtent" .


###  http://schema.landrs.org/producedby
gf:producedby rdf:type owl:ObjectProperty ;
              rdfs:domain gf:DataSource ;
              rdfs:range gf:SensorPayload ;
              rdfs:label "producedBy" .


###  http://schema.landrs.org/provides
gf:provides rdf:type owl:ObjectProperty ;
            rdfs:domain gf:SensorPayload ;
            rdfs:range gf:DataSource ;
            rdfs:label "provides" .


###  http://schema.landrs.org/providesparticipantrole
gf:providesparticipantrole rdf:type owl:ObjectProperty ;
                           rdfs:domain gf:Flight ;
                           rdfs:range gf:Uav ;
                           rdfs:label "providesParticipantRole" .


###  http://schema.landrs.org/servestrajectory
landrs:servesTrajectory rdf:type owl:ObjectProperty ;
                    rdfs:domain gf:Trajectorysource ;
                    rdfs:range gf:Trajectoryobservationcollection ;
                    rdfs:label "servesTrajectory" .


#################################################################
#    Classes
#################################################################

###  http://www.w3.org/ns/dcat#Dataset
dcat:Dataset rdf:type owl:Class ;
             rdfs:label "dcat:Dataset" .


###  http://www.w3.org/ns/prov#Activity
prov:Activity rdf:type owl:Class ;
              rdfs:label "prov:Activity" .


###  http://www.w3.org/ns/prov#Association
prov:Association rdf:type owl:Class ;
                 rdfs:label "prov:Association" .


###  http://www.w3.org/ns/prov#Plan
prov:Plan rdf:type owl:Class ;
          rdfs:label "prov:Plan" .


###  http://www.w3.org/ns/prov#Role
prov:Role rdf:type owl:Class ;
          rdfs:label "prov:Role" .


###  https://schema.landrs.org/schema//Dataaquisition
landrs:DataAquisition rdf:type rdfs:Class ;
      rdfs:subClassOf prov:Activity ,
      rdfs:label "DataAquisition" .


###  https://schema.landrs.org/schema//Datacaptureplan
<https://schema.landrs.org/schema//Datacaptureplan> rdf:type owl:Class ;
                                                    rdfs:subClassOf <https://schema.landrs.org/schema//Plan> ,
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty <https://schema.landrs.org/schema//wasgeneratedby> ;
                                                                      owl:someValuesFrom <https://schema.landrs.org/schema//Datacaptureplanningactivity>
                                                                    ] ;
                                                    rdfs:label "DataCapturePlan" .


###  https://schema.landrs.org/schema//Datacaptureplanningactivity
<https://schema.landrs.org/schema//Datacaptureplanningactivity> rdf:type owl:Class ;
                                                                rdfs:subClassOf prov:Activity ;
                                                                rdfs:label "DataCapturePlanningActivity" .


###  https://schema.landrs.org/schema//Dataplanassociation
<https://schema.landrs.org/schema//Dataplanassociation> rdf:type owl:Class ;
                                                        rdfs:subClassOf prov:Association ,
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty <https://schema.landrs.org/schema//hadplan> ;
                                                                          owl:someValuesFrom <https://schema.landrs.org/schema//Datacaptureplan>
                                                                        ] ;
                                                        rdfs:label "DataPlanAssociation" .


###  https://schema.landrs.org/schema//Flight
<https://schema.landrs.org/schema//Flight> rdf:type owl:Class ;
                                           rdfs:subClassOf prov:Activity ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <https://schema.landrs.org/schema//qualifiedassociation> ;
                                                             owl:someValuesFrom <https://schema.landrs.org/schema//Flightplanassociation>
                                                           ] ;
                                           rdfs:label "Flight" .


###  https://schema.landrs.org/schema//Flightplan
<https://schema.landrs.org/schema//Flightplan> rdf:type owl:Class ;
                                               rdfs:subClassOf <https://schema.landrs.org/schema//Plan> ,
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty <https://schema.landrs.org/schema//wasgeneratedby> ;
                                                                 owl:someValuesFrom <https://schema.landrs.org/schema//Flightplanningactivity>
                                                               ] ;
                                               rdfs:label "FlightPlan" .


###  https://schema.landrs.org/schema//Flightplanassociation
<https://schema.landrs.org/schema//Flightplanassociation> rdf:type owl:Class ;
                                                          rdfs:subClassOf prov:Association ,
                                                                          [ rdf:type owl:Restriction ;
                                                                            owl:onProperty <https://schema.landrs.org/schema//hadplan> ;
                                                                            owl:someValuesFrom <https://schema.landrs.org/schema//Flightplan>
                                                                          ] ;
                                                          rdfs:label "FlightPlanAssociation" .


###  https://schema.landrs.org/schema//Flightplanningactivity
<https://schema.landrs.org/schema//Flightplanningactivity> rdf:type owl:Class ;
                                                           rdfs:subClassOf prov:Activity ;
                                                           rdfs:label "FlightPlanningActivity" .


###  https://schema.landrs.org/schema//Pilot
<https://schema.landrs.org/schema//Pilot> rdf:type owl:Class ;
                                          rdfs:subClassOf <https://schema.landrs.org/schema//Role> ;
                                          rdfs:label "Pilot" .


###  https://schema.landrs.org/schema//Plan
<https://schema.landrs.org/schema//Plan> rdf:type owl:Class ;
                                         rdfs:subClassOf dcat:Dataset ,
                                                         prov:Plan ;
                                         rdfs:comment "a description having an explicit goal, to be achieved by executing the plan" ;
                                         rdfs:label "Plan" .


###  https://schema.landrs.org/schema//Role
<https://schema.landrs.org/schema//Role> rdf:type owl:Class ;
                                         rdfs:subClassOf prov:Role ;
                                         rdfs:label "Role" .

###  http://schema.landrs.org/DataSource
gf:DataSource rdf:type owl:Class ;
              rdfs:subClassOf dcat:DataService ;
              rdfs:comment "Any source of observational data from a sensor or sensor system hosted by a drone and is provided by some software API or API service" ;
              rdfs:label "Data Source" .


###  http://schema.landrs.org/Dataaquisition
gf:Dataaquisition rdf:type owl:Class ;
                  rdfs:label "DataAquisition" .


###  http://schema.landrs.org/Datacaptureplan
gf:Datacaptureplan rdf:type owl:Class ;
                   rdfs:label "DataCapturePlan" .


###  http://schema.landrs.org/Dataplanassociation
gf:Dataplanassociation rdf:type owl:Class ;
                       rdfs:label "DataPlanAssociation" .


###  http://schema.landrs.org/Event
gf:Event rdf:type owl:Class ;
         rdfs:comment "Event from A Core Pattern for Events" ;
         rdfs:label "Event" .


###  http://schema.landrs.org/Flight
gf:Flight rdf:type owl:Class ;
          rdfs:subClassOf gf:Event ,
                          prov:Activity ;
          rdfs:label "Flight" .


###  http://schema.landrs.org/ObservationCollection
gf:ObservationCollection rdf:type owl:Class ;
                         rdfs:subClassOf dcat:Dataset ,
                                         prov:Entity ,
                                         ssn-ext:ObservationCollection ;
                         rdfs:label "Observation Collection" .


###  http://schema.landrs.org/SensorPayload
gf:SensorPayload rdf:type owl:Class ;
                 rdfs:subClassOf ssn:System ;
                 rdfs:comment "Hardware Component of a drone system that provides a data source" ;
                 rdfs:label "Sensor Payload" .


###  http://schema.landrs.org/Spatialtemporalaggregate
gf:Spatialtemporalaggregate rdf:type owl:Class ;
                            rdfs:subClassOf wgs:SpatialThing ,
                                            tagg:TemporalAggregate ;
                            rdfs:label "SpatialTemporalAggregate" .


###  http://schema.landrs.org/Trajectoryobservationcollection
gf:Trajectoryobservationcollection rdf:type owl:Class ;
                                   rdfs:subClassOf gf:ObservationCollection ,
                                                   gf:Spatialtemporalaggregate ;
                                   rdfs:label "TrajectoryObservationCollection" .


###  http://schema.landrs.org/Trajectorysource
gf:Trajectorysource rdf:type owl:Class ;
                    rdfs:subClassOf gf:DataSource ;
                    rdfs:label "TrajectorySource" .


###  http://schema.landrs.org/Uav
gf:Uav rdf:type owl:Class ;
       rdfs:label "UAV" .





