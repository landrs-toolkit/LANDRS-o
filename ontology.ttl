#### Classes Derived from Schema.org
landrs:Thing a rdfs:Class ;
  rdfs:label "Thing"@en ;
  rdfs:comment "The most generic type of item."@en ;
  schema:sameAs schema:Thing ;
  owl:equivalentClass schema:Thing ;
  owl:equivalentClass owl:Thing .

landrs:Event a rdfs:Class ;
    rdfs:label "Event"@en ;
    rdfs:comment "An event happening at a certain time and location, such as a concert, lecture, or festival. Ticketing information may be added via the <a class=\"localLink\" href=\"http://schema.org/offers\">offers</a> property. Repeated events may be structured as separate Event objects."@en ;
    rdfs:subClassOf schema:Thing ;
    owl:equivalentClass dctype:Event ;
    owl:equivalentClass schema:Event .

landrs:Intangible a rdfs:Class ;
    rdfs:label "Intangible" ;
    schema:sameAs schema:Intangible ;
    owl:equivalentClass schema:Intangible ;
    rdfs:comment "A utility class that serves as the umbrella for a number of 'intangible' things such as quantities, structured values, etc." ;
    rdfs:subClassOf schema:Thing .

landrs:Product a rdfs:Class ;
  rdfs:label "Product"@en ;
  dcterms:source <http://www.w3.org/wiki/WebSchemas/SchemaDotOrgSources#source_GoodRelationsTerms> ;
  rdfs:comment "Any offered product or service. For example: a pair of shoes; a concert ticket; the rental of a car; a haircut; or an episode of a TV show streamed online."@en ;
  owl:equivalentClass schema:Product ;
  schema:sameAs schema:Product ;
  rdfs:subClassOf landrs:Thing ;
  rdfs:subClassOf schema:Thing .

landrs:ProductModel a rdfs:Class ;
  rdfs:label "ProductModel" ;
  dcterms:alternative:source <http://www.w3.org/wiki/WebSchemas/SchemaDotOrgSources#source_GoodRelationsClass> ;
  rdfs:comment "A datasheet or vendor specification of a product (in the sense of a prototypical description)." ;
  rdfs:subClassOf schema:Product ;
  rdfs:subClassOf landrs:Product ;
  schema:sameAs schema:ProductModel ;
  owl:equivalentClass schema:ProductModel .

landrs:QuantitativeValue a rdfs:Class ;
    rdfs:label "QuantitativeValue" ;
    schema:sameAs schema:QuantitativeValue ;
    owl:equivalentClass schema:QuantitativeValue ;
    dcterms:source <http://www.w3.org/wiki/WebSchemas/SchemaDotOrgSources#source_GoodRelationsClass> ;
    rdfs:comment "A point value or interval for product characteristics and other purposes." ;
    rdfs:subClassOf schema:StructuredValue ;
    rdfs:subClassOf landrs:StructuredValue .

landrs:PropertyValue a rdfs:Class ;
    rdfs:label "PropertyValue" ;
    owl:equivalentClass schema:PropertyValue ;
    schema:sameAs schema:PropertyValue ;
    dcterms:source <http://www.w3.org/wiki/WebSchemas/SchemaDotOrgSources#source_GoodRelationsClass> ;
    rdfs:comment """A property-value pair, e.g. representing a feature of a product or place. Use the 'name' property for the name of the property. If there is an additional human-readable version of the value, put that into the 'description' property.<br/><br/>
Always use specific schema.org properties when a) they exist and b) you can populate them. Using PropertyValue as a substitute will typically not trigger the same effect as using the original, specific property.""" ;
    rdfs:subClassOf schema:StructuredValue ;
    rdfs:subClassOf landrs:StructuredValue .

landrs:StructuredValue a rdfs:Class ;
    rdfs:label "StructuredValue" ;
    schema:sameAs schema:StructuredValue ;
    owl:equivalentClass schema:StructuredValue ;
    rdfs:comment "Structured values are used when the value of a property has a more complex structure than simply being a textual value or a reference to another thing." ;
    rdfs:subClassOf schema:Intangible .


landrs:Vehicle a rdfs:Class ;
  rdfs:label "Vehicle"@en ;
  rdfs:comment "A vehicle is a device that is designed or used to transport people or cargo over land, water, air, or through space."@en ;
  rdfs:subClassOf schema:Product ;
  rdfs:subClassOf landrs:Product ;
  schema:sameAs schema:Vehicle ;
  owl:equivalentClass schema:Vehicle .



